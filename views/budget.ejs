    <script>
        function budget() {
          // Get the income entered by the user
          var income = parseFloat(document.getElementById('income').value);
    
          // Define the percentages for saving, spending, and investing
          var savingPercentage = 0.3; // 30%
          var spendingPercentage = 0.5; // 50%
          var investingPercentage = 0.2; // 20%
    
          // Calculate the amounts based on the percentages
          var savingAmount = income * savingPercentage;
          var spendingAmount = income * spendingPercentage;
          var investingAmount = income * investingPercentage;
    
          // Display the allocated amounts
          document.getElementById('saving').innerText = 'Your amount allocated to Saving: $' + savingAmount.toFixed(2);
          document.getElementById('spending').innerText = 'Your amount allocated to Spending: $' + spendingAmount.toFixed(2);
          document.getElementById('investing').innerText = 'Your amount allocated to Investing: $' + investingAmount.toFixed(2);
          document.getElementById('investtips').innerText = 'Below we will provide you with some investing tips to be financially free! '
    
          // Generate and display three options
          generateOptions();
        }
    
        function generateOptions() {
          // Get the options container
          var optionsContainer = document.getElementById('optionsContainer');
          optionsContainer.innerHTML = ''; // Clear previous options
    
          // Sample data for options (image URL, title, and description)
          var optionsData = [
            {
              image: 'https://www.bioworld.com/ext/resources/Stock-images/Money/stock-prices-up.png?1588794938', // Replace with your image URL
              title: 'Stocks',
              description: 'Includes buying stocks like the S&P 500',
              link: '/stocks.html' // Replace with your destination URL for Option 1
            },
            {
              image: 'https://images.pexels.com/photos/106399/pexels-photo-106399.jpeg?auto=compress&cs=tinysrgb&dpr=1&w=500', // Replace with your image URL
              title: 'Real Estate',
              description: 'Highest return on investment if done right',
              link: '/realestate.html' // Replace with your destination URL for Option 2
            },
            {
              image: 'https://image.cnbcfm.com/api/v1/image/106539503-1589561901488gettyimages-580502927.jpeg?v=1687888038', // Replace with your image URL
              title: 'High-Yield saving accounts',
              description: 'Provides higher rates of return than traditional checking accounts',
              link: '/savingaccount.html' // Replace with your destination URL for Option 3
            }
          ];
    
          // Create and append options elements to the container
          optionsData.forEach(function(option) {
            var optionDiv = document.createElement('div');
            optionDiv.classList.add('option');
    
            // Create an anchor tag (link) around the image
            var linkElement = document.createElement('a');
            linkElement.href = option.link;
    
            var imageElement = document.createElement('img');
            imageElement.src = option.image;
            imageElement.classList.add('option-image'); // Add a class for styling
    
            var titleElement = document.createElement('h3');
            titleElement.textContent = option.title;
    
            var descriptionElement = document.createElement('p');
            descriptionElement.textContent = option.description;
    
            // Append elements to the container
            optionDiv.appendChild(titleElement);
            linkElement.appendChild(imageElement);
            optionDiv.appendChild(linkElement);
            optionDiv.appendChild(descriptionElement);
    
            optionsContainer.appendChild(optionDiv);
          });
        }
      </script>

